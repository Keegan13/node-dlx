{
  "name": "dancing-links",
  "version": "1.0.0",
  "description": "",
  "main": "built/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf built",
    "build": "npm run clean && tsc -p .",
    "prepare": "npm run build",
    "test": "npm run test-unit",
    "test-watch": "npm run test-unit -- --watch",
    "test-unit": "NODE_ENV=test mocha --compilers ts-node/register 'test/unit/**/*.spec.js' 'test/unit/**/*.spec.ts'",
    "cover": "nyc npm run test-unit",
    "coverage": "npm run cover && nyc report --reporter=text-lcov > coverage.lcov",
    "coverage-report": "npm run coverage && codecov && rm -rf ./nyc_output && rm coverage.lcov",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "nyc": {
    "per-file": true,
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.spec.js",
      "test/**/*.spec.ts",
      "test/**/*.js",
      "test/**/*.ts",
      "node_modules/**/*.js",
      "node_modules/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "all": true,
    "sourceMap": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/lodash": "^4.14.85",
    "@types/mocha": "^2.2.44",
    "chai": "^2.3.0",
    "mocha": "^5.0.0",
    "nyc": "^11.3.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1"
  }
}
